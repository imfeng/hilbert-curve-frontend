{
  "name": "hilbert-chart",
  "version": "1.5.0",
  "description": "A hilbert space-filling curve D3 chart for representing one-dimensional lengths on a two-dimensional space.",
  "unpkg": "dist/hilbert-chart.min.js",
  "main": "dist/hilbert-chart.common.js",
  "module": "dist/hilbert-chart.module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/hilbert-chart.git"
  },
  "homepage": "https://github.com/vasturiano/hilbert-chart",
  "keywords": [
    "hilbert",
    "space filling",
    "curve",
    "d3",
    "chart"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/hilbert-chart/issues"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/hilbert-chart.js -o dist/hilbert-chart.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*",
    "example/**/*"
  ],
  "dependencies": {
    "accessor-fn": "^1.2.2",
    "d3-axis": "^1.0.12",
    "d3-hilbert": "^1.1.0",
    "d3-scale": "^3.0.1",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.4.0",
    "d3-tip": "^0.9.1",
    "d3-zoom": "^1.7.3",
    "gsap": "^2.1.3",
    "heatmap.js": "^2.0.5",
    "kapsule": "^1.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.18.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-watch": "^4.3.1",
    "terser": "^4.1.2"
  }
}
